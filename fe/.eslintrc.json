{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, {
    "extensions": [".js", ".jsx"] }],
    "react/react-in-jsx-scope": ["off"],
    "linebreak-style": ["off"],
    "import/prefer-default-export": "off",
    "react/jsx-no-bind": ["off"]
  }
}
